

<header class="page-title pt-large pt-light pt-parallax pt-plax-lg-light" data-stellar-background-ratio="0.4">
	<div class="container">
			<div class="row">

				<div class="col-sm-6">
					<h1>Projects</h1>
					<span class="subheading">Some Of Our Work</span>
				</div>
				<ol class="col-sm-6 text-right breadcrumb">
					<li><%= link_to "Home", root_path %></li>
					<li class="active"><%= link_to "Projects", projects_path %></li>
				</ol>

			</div>
		</div>
</header>


			<section class="container section portfolio-layout portfolio-1col-boxed">

				<div class="row">
					<div id="pfolio">

						<!-- Item 1 -->
						  <% @projects.each do |project| %>
						<figure class="portfolio-item print">
							<div class="col-md-7 no-gap img-wrapper">
								<%= image_tag project.thumbnail.url(:medium) %>
							</div>
							<figcaption class="col-md-5 no-gap">
								<h3><%= project.name %></h3>
								<p><%= raw(project.description) %>.</p>

								<%= link_to 'Veiw projects', project, class: "btn-ghost btn-small view-btn" %>
							</figcaption>
						</figure><!-- / .portfolio-item -->

						<% end %>
					</div><!-- / #pfolio -->
				</div><!-- / .row -->
			</section><!-- / .container -->



			<!-- ========== CTA - Contact Link ========== -->
			<div class="cta-link">
		    <div class="bg-overlay">

		    

		    </div>
		  </div><!-- / .dark-bg .cta-link -->



<%= link_to 'New Project', new_project_path %>
